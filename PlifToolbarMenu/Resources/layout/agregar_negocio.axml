<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:minWidth="25px"
        android:minHeight="25px">
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout0"
            android:layout_marginTop="50dp"
            android:layout_marginBottom="30dp"
            android:padding="10dp">
        <!-- Nombre, descripción y sitio web -->
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:id="@+id/sample_main_layout"
                android:background="@drawable/fondoinfo">
                <TextView
                    android:text="Datos Generales"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/sobreeste"
                    android:textColor="#717A8F"
                    android:textSize="22sp"
                    android:layout_marginBottom="10dp" />
                <TextView
                    android:text="Nombre:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/sobreeste"
                    android:textColor="#717A8F"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp" />
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/nombre"
                    android:background="@drawable/back"
                    android:layout_marginBottom="5dp" />
                <TextView
                    android:text="Descripción:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/nombre"
                    android:textColor="#717A8F"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp" />
                <EditText
                    android:inputType="textMultiLine"
                    android:id="@+id/descripcion"
                    android:lines="8"
                    android:minLines="6"
                    android:gravity="top|left"
                    android:maxLines="10"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:scrollbars="vertical"
                    android:background="@drawable/back"
                    android:padding="5dp" />
                <TextView
                    android:text="Sitio web:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/sobreeste"
                    android:textColor="#717A8F"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp" />
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/sitioweb"
                    android:background="@drawable/back"
                    android:layout_marginBottom="5dp" />
            </LinearLayout>
        <!-- Termina Nombre, descripción y sitio web -->
        <!-- Datos generales-->
            <LinearLayout

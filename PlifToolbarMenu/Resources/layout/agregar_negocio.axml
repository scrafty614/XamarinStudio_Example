                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:id="@+id/sample_main_layout"
                android:background="@drawable/fondoinfo">
                <TextView
                    android:text="Datos Generales"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/sobreeste"
                    android:textColor="#717A8F"
                    android:textSize="22sp"
                    android:layout_marginBottom="10dp" />
                <TextView
                    android:text="Si te encuentras en el negocio que quieres agregar, haz click aquí para llenar algunos datos por ti."
                    android:textColor="#717A8F"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView1"
                    android:layout_marginTop="10dp" />
                <Button
                    android:text="Localizando..."
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/imhere"
                    android:enabled="false"
                    android:textColor="#FFFFFF"
                    android:background="@drawable/addlike2" />
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="250dp"
                    android:id="@+id/fragmentmap"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_marginTop="5dp"
                    android:visibility="gone">
                    <fragment
                        android:id="@+id/map"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        class="com.google.android.gms.maps.MapFragment" />
                </LinearLayout>
                <ProgressBar
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/esperadatos"
                    style="?android:attr/progressBarStyleSmall" />
                <TextView
                    android:text="Región:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/regiontext"
                    android:textColor="#717A8F"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="10dp" />
                <RadioGroup
                    android:id="@+id/region"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp">
                    <RadioButton
                        android:id="@+id/regiondurango"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Durango"
                        android:checked="true"
                        android:textColor="#717A8F" />
                    <RadioButton
                        android:id="@+id/regionmazatlan"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Mazatlán"
                        android:textColor="#717A8F" />
                    <RadioButton
                        android:id="@+id/regionzacatecas"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Zacatecas"
                        android:textColor="#717A8F" />
                    <RadioButton
                        android:id="@+id/regiontorreon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Torreón"
                        android:textColor="#717A8F" />
                </RadioGroup>
                <TextView
                    android:text="Email:"
                    android:inputType="textEmailAddress"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/sobreeste"
                    android:textColor="#717A8F"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp" />
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/email"
                    android:background="@drawable/back"
                    android:layout_marginBottom="5dp" />
                <TextView
                    android:text="Categoría:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/sobreeste"
                    android:textColor="#717A8F"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp"
                    android:visibility="gone" />
                <Spinner
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:id="@+id/categorias"
                    android:background="@drawable/back"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:layout_marginBottom="10dp"
                    android:visibility="gone" />
                <TextView
                    android:text="Teléfono:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/telefono"
                    android:inputType="number"
                    android:maxLength="10"
                    android:textColor="#717A8F"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp" />
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/telefonoedit"
                    android:background="@drawable/back"
                    android:layout_marginBottom="5dp" />
                <TextView
                    android:text="País:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/telefono"
                    android:textColor="#717A8F"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp" />
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/pais"
                    android:background="@drawable/back"
                    android:layout_marginBottom="5dp" />
                <TextView
                    android:text="Ciudad:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/telefono"
                    android:textColor="#717A8F"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp" />
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/ciudad"
                    android:background="@drawable/back"
                    android:layout_marginBottom="5dp" />
                <TextView
                    android:text="Colonia:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/telefono"
                    android:textColor="#717A8F"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp" />
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/colonia"
                    android:background="@drawable/back"
                    android:layout_marginBottom="5dp" />
                <TextView
                    android:text="Calle:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/telefono"
                    android:textColor="#717A8F"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp" />
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/calle"
                    android:background="@drawable/back"
                    android:layout_marginBottom="5dp" />
                <TextView
                    android:text="Número"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/telefono"
                    android:textColor="#717A8F"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp" />
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/numero"
                    android:inputType="number"
                    android:maxLength="5"
                    android:background="@drawable/back"
                    android:layout_marginBottom="5dp" />
                <TextView
                    android:text="Código Postal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/telefono"
                    android:textColor="#717A8F"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp" />
                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/codigopostal"
                    android:inputType="number"
                    android:maxLength="5"
                    android:background="@drawable/back"
                    android:layout_marginBottom="5dp" />
                <TextView
                    android:text="Referencias:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/nombre"
                    android:textColor="#717A8F"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp" />
                <EditText
                    android:inputType="textMultiLine"
                    android:id="@+id/referencias"
                    android:lines="8"
                    android:minLines="6"
                    android:gravity="top|left"
                    android:maxLines="10"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:scrollbars="vertical"
                    android:background="@drawable/back"
                    android:padding="5dp" />
                <TextView
                    android:text="Palabras Clave:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tagsnegociotit"
                    android:textColor="#717A8F"
                    android:textSize="18sp"
                    android:layout_marginBottom="5dp" />

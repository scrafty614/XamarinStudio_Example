                        <android.support.v7.widget.SwitchCompat
                            android:id="@+id/lunes_ac"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="20"
                            android:text=""
                            app:showText="false" />
                    </LinearLayout>
